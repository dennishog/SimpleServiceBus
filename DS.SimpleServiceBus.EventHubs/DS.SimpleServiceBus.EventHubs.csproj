<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Dennis Hogstr√∂m</Authors>
    <Company>Dennis Software</Company>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dennishog/SimpleServiceBus</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dennishog/SimpleServiceBus</RepositoryUrl>
    <Version>1.1.0</Version>
    <Description>Package for adding EventHubs communication through SimpleServiceBus</Description>
    <PackageTags>EventHubs, Servicebus</PackageTags>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.EventHubs" Version="2.0.0" />
    <PackageReference Include="Microsoft.Azure.EventHubs.Processor" Version="2.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DS.SimpleServiceBus\DS.SimpleServiceBus.csproj" />
  </ItemGroup>

</Project>
